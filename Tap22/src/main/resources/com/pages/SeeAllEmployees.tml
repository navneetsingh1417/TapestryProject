<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">
<body>
<t:pagelink page="Dashboard">Dashboard</t:pagelink>
<br></br>

<t:grid include="id,name,username" source="employees" row="employee">
            <p:idCell>
                <t:pagelink page="ViewEmployee" context="employee.id">${employee.id}</t:pagelink>
            </p:idCell>

</t:grid>
	
</body>
</html>